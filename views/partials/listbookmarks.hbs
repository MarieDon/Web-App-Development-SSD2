{{#each bookmark}}
  <section class="ui segment">
    <h2 class="ui header x">
      {{title}}
    </h2>
    
    <a href="/bookmark/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deleteBookmark/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}